{
  "name": "@storybook-community/storybook-addon-cookie",
  "version": "0.0.0-semantically-released",
  "description": "Set document.cookie in your stories.",
  "author": "hwookim <hwookim.dev@gmail.com>",
  "keywords": [
    "storybook-addon",
    "cookie",
    "mocking",
    "data-state"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/storybook-community/storybook-addon-cookie"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./preview": {
      "types": "./dist/preview.d.ts",
      "default": "./dist/preview.js"
    },
    "./manager": "./dist/manager.js",
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "*.js",
    "*.d.ts"
  ],
  "scripts": {
    "build": "tsup",
    "build-storybook": "storybook build",
    "build:watch": "pnpm build --watch",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint": "eslint --cache .",
    "lint:fix": "pnpm lint --fix",
    "pack": "pnpm pack --out storybook-addon-cookie-$(date +%s).tgz",
    "prebuild": "node -e \"fs.rmSync('./dist', { recursive: true, force: true })\"",
    "prerelease": "zx scripts/prepublish-checks.js",
    "release": "pnpm build && pnpm semantic-release",
    "release:canary": "pnpm build && auto canary",
    "start": "run-p build:watch \"storybook --quiet\"",
    "storybook": "storybook dev -p 6006",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@storybook/icons": "^1.4.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/js": "^9.36.0",
    "@storybook/addon-docs": "next",
    "@storybook/react-vite": "next",
    "@types/node": "^20",
    "@types/react": "^18.2.65",
    "@types/react-dom": "^18.2.21",
    "@vitejs/plugin-react": "^4.7.0",
    "auto": "^11.3.0",
    "boxen": "^8.0.1",
    "commitlint": "^20.0.0",
    "dedent": "^1.7.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.3",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.6.2",
    "prompts": "^2.4.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "semantic-release": "^24.2.9",
    "storybook": "next",
    "ts-dedent": "^2.2.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.46.2",
    "vite": "^7.0.5",
    "zx": "^8.7.1"
  },
  "peerDependencies": {
    "@storybook/addon-docs": "^10.0.0",
    "storybook": "^10.0.0"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true,
    "registry": "https://registry.npmjs.org/"
  },
  "bundler": {
    "managerEntries": [
      "src/manager.tsx"
    ],
    "previewEntries": [
      "src/preview.ts",
      "src/index.ts"
    ]
  },
  "packageManager": "pnpm@10.7.0",
  "engines": {
    "node": ">=22.19.0"
  },
  "storybook": {
    "displayName": "Cookie (community version)",
    "supportedFrameworks": [
      "supported-frameworks"
    ],
    "icon": "https://avatars.githubusercontent.com/u/216219414?s=200&v=4"
  }
}
